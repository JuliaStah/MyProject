pdateCart();\n       }\n\t},\n\t\n    plus(){\n\t    this.localQuantity ++;\n\t    this.debounceUpdateCart()\n\t},\n\n    debounceUpdateCart() {\n      if (this.debounceTimer) clearTimeout(this.debounceTimer);\n      this.debounceTimer = setTimeout(() => {\n        this.sendCartUpdate();\n      }, 1000);\n    },\n\n    async sendCartUpdate() {\n        this.isLoadingCart = true;\n        try {\n            if (this.localQuantity <= 0) {\n              await window.digiLayer.deleteCartItem(this.computedSkuProduct.id);\n            } else {\n              await window.digiLayer.changeCartItem(this.computedSkuProduct.id, this.localQuantity);\n            }\n            this.updateCartState();\n        } finally {\n            this.isLoadingCart = false;\n        }\n    },\n\t\n\tasync updateCartState() {\n\t\t\tconst cartState =\n\t\t\t\ttypeof window !== \"undefined\" &&\n\t\t\t\ttypeof window.digiLayer !== \"undefined\" &&\n\t\t\t\ttypeof window.digiLayer.cartState !== \"undefined\"\n\t\t\t\t\t? await window.digiLayer.cartState()\n\t\t\t\t\t: {};\n\t\tthis.cartItemState = cartState;\n        this.localQuantity = this.quantity;\n\t},\n\t\n\tchangeActiveSku(id){\n\t    this.currentSkuId = id;\n\t},\n\t\n\tcheckScrollPosition() {\n      if (!this.$refs.skusContainer) return;\n      \n      const container = this.$refs.skusContainer;\n      this.isScrollStart = container.scrollLeft <= 0;\n      this.isScrollEnd = container.scrollLeft + container.offsetWidth >= container.scrollWidth;\n    },\n\n    scrollToDirection(direction) {\n      if (!this.$refs.skusContainer || !this.$refs.skuButtons?.[0]) return;\n      \n      const container = this.$refs.skusContainer;\n      const buttonWidth = this.$refs.skuButtons[0].offsetWidth;\n      const scrollAmount = buttonWidth + 8; // 8px это gap между кнопками\n      \n      const newScrollPosition = direction === 'next' \n        ? container.scrollLeft + scrollAmount\n        : container.scrollLeft - scrollAmount;\n        \n      container.scrollTo({\n        left: newScrollPosition,\n        behavior: 'smooth'\n      });\n    },\n\n  enableDragScroll(element) {\n  // Не вешаем drag-scroll на мобильных\n  if (!element) return;\n  if (this.Device?.isMobile || /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {\n    element.addEventListener('scroll', () => {\n      this.checkScrollPosition();\n    });\n    return;\n  }\n\n  let isDown = false;\n  let startX;\n  let scrollLeft;\n\n  element.addEventListener('mousedown', (e) => {\n    isDown = true;\n    element.classList.add('dragging');\n    startX = e.pageX - element.getBoundingClientRect().left;\n    scrollLeft = element.scrollLeft;\n    e.preventDefault(); // Отключаем выделение текста\n  });\n\n  element.addEventListener('mouseleave', () => {\n    isDown = false;\n    element.classList.remove('dragging');\n  });\n\n  element.addEventListener('mouseup', () => {\n    isDown = false;\n    element.classList.remove('dragging');\n  });\n\n   element.addEventListener('mousemove', (e) => {\n    if (!isDown) return;\n    e.preventDefault(); // Отключаем выделение текста\n    const x = e.pageX - element.getBoundingClientRect().left;\n    const walk = x - startX;\n    element.scrollLeft = scrollLeft - walk;\n   });\n\n   // Добавляем слушатель события scroll\n   element.addEventListener('scroll', () => {\n    this.checkScrollPosition();\n   });\n  },\n    checkScroll() {\n      setTimeout(() => {\n        const container = this.$refs.skusContainer;\n        if (!container) return;\n\n        // Проверяем, есть ли скролл\n        this.hasScroll = container.scrollWidth > container.clientWidth;\n\n        // Проверяем позицию скролла\n        this.isScrollStart = container.scrollLeft <= 0;\n        this.isScrollEnd = container.scrollLeft + container.offsetWidth >= container.scrollWidth;\n      }, 0);\n    },\n    async changeActiveSku(id) {\n        this.currentSkuId = id;\n        // Обновляем состояние корзины после смены SKU\n        await this.updateCartState();\n    },\n\n    parseWeightOrSize(value) {\n      if (!value) return { type: 'unknown', order: 99999, raw: value };\n\n      let v = value.replace(/\\s/g, '').toLowerCase();\n\n      // Если диапазон (например, 1,5-4кг или 300-500г/гр)\n      let weightMatch = v.match(/^(\\d+[.,]?\\d*)-(\\d+[.,]?\\d*)(кг|гр|г)$/);\n      if (weightMatch) {\n        // Берём минимальное значение диапазона\n        v = weightMatch[1] + weightMatch[3];\n      }\n\n      // Килограммы\n      if (v.endsWith('кг')) {\n        const num = parseFloat(v.replace('кг', '').replace(',', '.'));\n        return { type: 'weight', order: num * 1000, raw: value }; // всегда в граммах!\n      }\n      // Граммы (г или гр)\n      if (v.endsWith('гр') || v.endsWith('г')) {\n        const num = parseFloat(v.replace(/гр?$/, '').replace(',', '.'));\n        return { type: 'weight', order: num, raw: value };\n      }\n\n      // Размеры\n      const sizeOrder = [\n        'xxs', 'xs', 's', 'm', 'l', 'xl', 'xxl', '2xl', '3xl', '4xl', '5xl'\n      ];\n      const sizeIndex = sizeOrder.indexOf(v);\n      if (sizeIndex !== -1) {\n        return { type: 'size', order: 100000 + sizeIndex, raw: value };\n      }\n      // Размеры с цифрой впереди (например, 2xl)\n      const matchNumSize = v.match(/^(\\d+)xl$/);\n      if (matchNumSize) {\n        return { type: 'size', order: 100000 + sizeOrder.length + parseInt(matchNumSize[1]), raw: value };\n      }\n\n      // fallback: сортировка по алфавиту\n      return { type: 'unknown', order: 200000 + v.charCodeAt(0), raw: value };\n    },\n\t\n},\n    computed: {\n\t\n    isRating() {\n        const rating = this.computedProduct.attributes?.['rating']?.[0]  ?? ''\n        return !!rating ? Number(parseFloat(rating).toFixed(1)) : ''\n    },\n    \n    isReviews() {\n      return this.computedProduct.attributes?.['reviews']?.[0] ?? ''\n    },\t\n\t\t\n\tquantity() {\n\t    // Также обновляем quantity для текущего SKU\n        return this.cartItemState ? Number(this.cartItemState[this.currentSkuId]) || 0 : 0;\n\t},\n\t\n\tisAdded() {\n        // Проверяем количество для текущего выбранного SKU\n        return this.cartItemState ? Number(this.cartItemState[this.currentSkuId]) || 0 : 0;\n    },\n\t\t\n    customSkuAttributes(){\n      const attrs = this.computedProduct?.attributes?.['sku_id']?.filter((el) => !el.startsWith(\"0 г|\")).map(el =>  el = {value: el?.split('|')?.[0], id: el?.split('|')?.[1]}) || []\n      return attrs.sort((a, b) => this.parseWeightOrSize(a.value).order - this.parseWeightOrSize(b.value).order)\n    },\n    \n\tcomputedSkuProduct() {\n\t\treturn this.computedProduct?.skus?.filter(item => item.id === this.currentSkuId)?.[0] ?? this.computedProduct\n\t},\n\t  computedTarget() {\n      return this.openInNewTab ? '_blank' : '_self';\n    },\n    computedRel() {\n      return this.openInNewTab ? 'noopener noreferrer' : null;\n    },\n    \n  },\n  \n  mounted() {\n    this.updateCartState();\n\n    this.$nextTick(() => {\n     if (this.$refs.skusContainer) {\n      this.enableDragScroll(this.$refs.skusContainer);\n      this.checkScroll();\n      window.addEventListener('resize', () => {\n        this.checkScroll();\n      });\n\n      // --- Только stopPropagation, без preventDefault ---\n      const el = this.$refs.skusContainer;\n      el.addEventListener('touchmove', function(e) {\n        e.stopPropagation();\n      }, { passive: true });\n      // --- конец блока ---\n     }\n   });\n\n    setTimeout(() => {\n      this.checkScroll();\n    }, 100);\n  },\n  \n  beforeDestroy() {\n    window.removeEventListener('resize', this.checkScroll);\n  },\n  \n  watch: {\n    customSkuAttributes: {\n        immediate: true,\n        handler() {\n            this.$nextTick(() => {\n                this.checkScroll();\n            });\n        }\n    }\n  },\n\n}"}],imageSearch:{},recsSimilar:{},recsPortal:{}},mvt:[{id:57557,name:"AC taps click event - 20.06.25 Kov